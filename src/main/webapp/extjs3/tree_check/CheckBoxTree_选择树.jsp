<%@ page language="java" contentType="text/html; charset=UTF8" %><html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF8"><title>[1]Ext2.x CheckBoxTree(xiexueze)</title><link rel="stylesheet" type="text/css" href="<%=request.getContextPath() %>/js/extjs-3.3.1-min/resources/css/ext-all.css"/><script type="text/javascript" src="<%=request.getContextPath() %>/js/extjs-3.3.1-min/adapter/ext/ext-base.js"></script><script type="text/javascript" src="<%=request.getContextPath() %>/js/extjs-3.3.1-min/ext-all.js"></script><script type="text/javascript" src="<%=request.getContextPath() %>/extjs3/ux/TreeCheckNodeUI.js"></script>	<script>		Ext.onReady(function(){		    var tree = new Ext.tree.TreePanel({		        applyTo:'tree-ct',		        width:568,		        height:300,		        checkModel: 'cascade',	//对树的级联多选   其他值:childCascade  parentCascade		        animate: false,		        autoScroll:true,		//滚动条,当树节点数据超过height时,出现滚动条.默认为false不出现滚动条.		        rootVisible: true, 		//为false则不显示根节点				loader: new Ext.tree.TreeLoader({					//dataUrl:'treedemo.action',		//同步					//dataUrl:'../ext_tree_data.json',	//同步					dataUrl:'../ext_tree_getNodes.jsp',	//异步,需要添加listener事件					listeners: {						beforeload: function(loader,node) {							this.baseParams.nodeid=node.id;						}	        		},					baseAttrs: {						uiProvider: Ext.ux.TreeCheckNodeUI //添加 uiProvider 属性					}		        }),		        root: new Ext.tree.AsyncTreeNode({		            id:'0',text:'根结点'		        })		    });			tree.getEl().center();		    tree.getRootNode().expand();		});	</script></head><body><p>1.选择树CheckBoxTree</p><div id="tree-ct"></body></html>