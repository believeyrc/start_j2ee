<%@ page contentType="text/html;charset=UTF-8" %><%@ include file="/common/taglibs.jsp" %><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><title>Fusion Chart Test</title><script type="text/javascript" src="${ctx}/js/Charts/FusionChartsXT-3.2.4/FusionCharts.js"></script><script type="text/javascript" src="${ctx}/js/jquery.js"></script><script type="text/javascript">$(document).ready(function(){	var type = '${type}';	var data = '${data}';	var chart;	if('json' == type){		//使用Flash，会在左上角出现FusionCharts Trial链接。		//如果不支持Flash，使用javascript展现。同时在右下角会出现FusionCharts链接。将FusionCharts.HC.js的text:"FusionCharts"改成""就不会出现链接了。		//JSON数据的setJSONData方法目前仅支持XT商业版本，Free版本暂不支持.		//flash和javascript的不同点是，flash在图表点击右键，弹出flash设置。		FusionCharts.setCurrentRenderer('javascript'); 			 	chart = new FusionCharts("${ctx}/js/Charts/FusionChartsXT-3.2.4/Column3D.swf", "myChartId", "800", "300", "0", "1");		chart.setJSONData(data);		}else{		chart = new FusionCharts("${ctx}/js/Charts/FusionChartsFree-1.2.3/FCF_Column3D.swf", "0", "800", "300");		chart.setDataXML(data);	}	chart.render('chartDiv');});	</script></head><body>	<div id="chartDiv" align="center"></div></body></html>