<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" ><sqlMap namespace="user" >	<typeAlias alias="User" type="com.xuyuan.ssi.model.User" />    <select id="getUserList" resultClass="java.util.HashMap" parameterClass="java.util.HashMap">        select id,username,password from user where 1=1        <isNotNull property="start">        	<isNotNull property="limit">			limit #start#,#limit#        	</isNotNull>        </isNotNull>    </select>    <select id="getUserCount" resultClass="java.lang.Integer">        select count(*) from user    </select></sqlMap>